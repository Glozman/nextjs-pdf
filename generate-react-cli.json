{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",

  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/TemplateName.tsx",
        "style": "templates/TemplateName.module.scss"
      },
      "path": "components",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    },
    "common": {
      "customTemplates": {
        "component": "templates/TemplateName.tsx",
        "style": "templates/TemplateName.module.scss"
      },
      "path": "components/common",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    }
  }
}